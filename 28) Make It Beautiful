#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){
        int n;
        cin>>n;
        vector<int> store(n,0);
        for (int i=0;i<n;i++){
            cin>>store[i];
        }
        if (store[0]==store[n-1]){
            cout<<"No"<<endl;
        }
        else{
            cout<<"Yes"<<endl;
            cout<<store[n-1]<<" ";
            cout<<store[0]<<" ";
            for (int i=n-2;i>=1;i--){
                cout<<store[i]<<" ";
            }
            cout<<endl;
        }

        // T.C:- O(n);
        // S.C:- O(n)
    }
    return 0;
}
