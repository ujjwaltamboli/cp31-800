#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){
        int n;
        cin>>n;
        vector<int> store(n,0);
        int mini=INT_MAX;
        for (int i=0;i<n;i++){//O(n)
            cin>>store[i];
        }
        if (store[0]==1){
            cout<<"Yes"<<endl;
        }
        else{
            cout<<"No"<<endl;
        }
        //T.C:- O(n);
        //S.C:- O(n);
    }
    return 0;
}
