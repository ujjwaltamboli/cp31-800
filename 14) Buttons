#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){
        int a,b,c;
        cin>>a>>b>>c;
        int anna=a+(c-c/2);
        int katie=b+c/2;
        if (anna>katie){
            cout<<"First"<<endl;
        }
        else{
            cout<<"Second"<<endl;
        }

        //T.C:- O(1);
        //S.C:- O(1);
    }
    return 0;
}
