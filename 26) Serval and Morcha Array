#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){
        int n;
        cin>>n;
        vector<int> store(n,0);
        for (int i=0;i<n;i++){
            cin>>store[i];
        }
        int mini=INT_MAX;
        for (int i=0;i<n;i++){
            for (int j=i+1;j<n;j++){
                mini=min(mini,__gcd(store[i],store[j]));
            }
        }
        if (mini<=2){
            cout<<"Yes"<<endl;
        }
        else{
            cout<<"No"<<endl;
        }

        //T.C.:- O(n2);
        //S.C.:- O(n);
    }
}
