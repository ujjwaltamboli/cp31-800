#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){
        int n;
        cin>>n;
        vector<int> store(n-1,0);
        int sum=0;
        for (int i=0;i<n-1;i++){
            cin>>store[i];
            sum=sum+store[i];
        }
        cout<<int(-1)*sum<<endl;

        //T.C:-O(n-1);
        //S.C:- O(n-1);
    }
    return 0;
}
