#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){
        long long n,k;
        cin>>n>>k;
        if (n%2!=0 && k%2==0){
            cout<<"No"<<endl;
        }
        else{
            cout<<"yes"<<endl;
        }
    }
    // T.C:- O(1);
    // S.C.:- O(1);

    return 0;
}
