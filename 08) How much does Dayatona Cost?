#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){
        int n,k;
        cin>>n>>k;
        vector<int> store(n,0);
        bool flag=false;
        for (int i=0;i<n;i++){// O(n)
            cin>>store[i];
            if (store[i]==k){
                flag=true;
            }
        }
        if (flag){
            cout<<"Yes"<<endl;
        }
        else{
            cout<<"No"<<endl;
        }

        //T.C:- O(n)
        // S.C :- O(n)
    }
    return 0;
}
