#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){
        int n;
        cin>>n;
        vector<int> store(n,0);
        for (int i=0;i<n;i++){
            cin>>store[i];
        }

        for (int i=0;i<n;i++){
            cout<<n+1-store[i]<<" ";
        }
        cout<<endl;
    }
    //T.C:- O(n);
    //S.C:- O(n);
    return 0;
}
